<div>
    <table class="table table-boldered">
        <h1>products</h1>
        <% @products.each do |f| %>
        <tr>
            <td>
                <%= f.title %>
            </td>
            <td>
                <%= f.description %>
            </td>
            <td>
                <%= f.created_at %>
            </td>
        </tr>
        <% end %>
    </table>

</div>
