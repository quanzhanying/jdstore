
<table class="table table-boldered">
    <thead>
        <tr>
            <td>#</td>
            <td>
                品名</td>
            <td>叙述</td>
            <td>库存</td>
            <td>价格</td>
            <td>选项</td>
            <td>创建日期</td>
        </tr>

    </thead>

  <% @products.each do |product| %>
  <tr>
    <td>
       <%= product .wage_lower_bound %>
    </td>
    <td>
      <%= link_to(product.title, product_path(product)) %>
    </td>
    <td>
       <%= product .description %>
    </td>
    <td>
       <%= product .wage_lower_bound %>
    </td>
    <td>
       <%= product.wage_upper_bound %>
    </td>
    

    <td>
      <%= product.created_at %>
    </td>
  </tr>
  <% end %>
</table>