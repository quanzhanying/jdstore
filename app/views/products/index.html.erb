<h2>Product List</h2>

<br><br>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>品名</th>
      <th>叙述</th>
      <th>库存</th>
      <th>价格</th>

      <th>创建日期</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.id %></td>
        <td><%= product.title %></td>
        <td><%= product.description %></td>
        <td><%= product.quantity %></td>
        <td><%= product.price %></td>
        
        <td><%= product.created_at %></td>
      </tr>
    <% end %>
  </tbody>


</table>
