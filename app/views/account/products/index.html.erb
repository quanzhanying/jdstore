<h2> 发起的众筹列表 </h2>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>标题</th>
      <th>描述</th>
      <th>发起者</th>
      <th>生成时间</th>

    </tr>
  </thead>
  <tbody>

    <% @products.each do |product| %>
    <tr>
      <td> <%= link_to(product.title, account_product_path(product)) %> </td>
      <td> <%= product.description %> </td>
      <td> <%= product.user.email %> </td>
      <td> <%= product.created_at.to_s(:long) %> </td>
    </tr>
    <% end %>

  </tbody>
</table>
