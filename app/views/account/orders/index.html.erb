<h2>Order List</h2>
<table class="table table-borderd">  
	<thead>
		<tr>
		<th>#</th>
		<th>Created_At</th>
		</tr>
	</thead>
	<tbody>
		<% @orders.each do |order| %>
			<tr>
				<td><%= link_to(order.id,order_path(order.token)) %></td>
				<td><%= order.created_at.to_s(:long) %></td>
			</tr>
		<% end %>
	</tbody>
</table>