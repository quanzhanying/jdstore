<h2> Order Lists </h2>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>Order Time</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
    <tr>
      <td> <%= link_to(order.id, order_path(order.token)) %></td>
      <td> <%= order.created_at.to_s(:long) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
